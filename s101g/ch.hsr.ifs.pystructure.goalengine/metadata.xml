<?xml version="1.0" encoding="UTF-8"?>
<flavor name="ch.hsr.ifs.pystructure.goalengine">

	<provider name="PyStructure" url="http://pystructure.ifs.hsr.ch" email=""/>

	<description>PyStructure GoalEngine Visualisation</description>

	<item-types>
		<module-types sep=".">
			<module-type name="evaluator"/>
		</module-types>
		<nested-types>
			<nested-type name="method"/>
			<nested-type name="attribute"/>
		</nested-types>
		<virtual-types>
			<virtual-type nature="root"/>
			<virtual-type nature="folder" name="package"/>
			<virtual-type nature="module" name="module"/>
			<virtual-type nature="bundle"/>
		</virtual-types>
	</item-types>

	<dependency-types>
		<dependency-type name="AttributeReferences"/>
		<dependency-type name="ClassReferences"/>
		<dependency-type name="FunctionReferences"/>
		<dependency-type name="MethodReferences"/>
		<dependency-type name="PossibleAttributeReferences"/>
		<dependency-type name="PossibleReferences"/>
		<dependency-type name="AbstractType"/>
		<dependency-type name="DefinitionType"/>
		<dependency-type name="ExpressionType"/>
		<dependency-type name="ReturnType"/>
		<dependency-type name="TupleElementType"/>
	</dependency-types>

</flavor>
