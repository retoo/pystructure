% Command for floating info boxes
% usage: \infobox{<title>}{<text>}
\usepackage{wrapfig}

\newcommand{\infobox}[2]{
  \begin{wrapfigure}{r}{6cm}
    \hspace{4pt}
    \fbox{
      \parbox[b]{5.4cm}{\smallskip \footnotesize {\bf #1} \smallskip
      
        #2
      
        \smallskip
      }
    }
  \end{wrapfigure}
}




